{
    "name": "simemotor-advisor",
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">20.0.0"
    },
    "scripts": {
        "ng": "ng",
        "start:prod": "ng serve -c production",
        "build:prod": "ng build -c production",
        "start:stag": "ng serve -c development",
        "build:stag": "ng build -c development --output-hashing=all",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "lint": "ng lint",
        "i18n:extract:en": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en-US.json --key-as-default-value --clean --sort --format json",
        "i18n:extract:ms": "ngx-translate-extract --input ./src --output ./src/assets/i18n/ms-MY.json --key-as-default-value --clean --sort --format json"
    },
    "private": true,
    "dependencies": {
        "@angular/cdk": "^19.2.19",
        "@angular/common": "^19.2.0",
        "@angular/compiler": "^19.2.0",
        "@angular/core": "^19.2.0",
        "@angular/forms": "^19.2.0",
        "@angular/material": "^19.2.19",
        "@angular/platform-browser": "^19.2.0",
        "@angular/platform-browser-dynamic": "^19.2.0",
        "@angular/router": "^19.2.0",
        "@ngx-translate/core": "^17.0.0",
        "@ngx-translate/http-loader": "^17.0.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.15.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^19.2.15",
        "@angular/cli": "^19.2.15",
        "@angular/compiler-cli": "^19.2.0",
        "@colsen1991/ngx-translate-extract-marker": "^3.0.1",
        "@tailwindcss/postcss": "^4.1.11",
        "@types/jasmine": "~5.1.0",
        "@vendure/ngx-translate-extract": "^10.0.1",
        "angular-eslint": "20.1.1",
        "eslint": "^9.29.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.3",
        "jasmine-core": "~5.6.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "prettier-eslint": "^16.4.2",
        "tailwindcss": "3.4",
        "typescript": "~5.7.2",
        "typescript-eslint": "8.34.1"
    }
}
